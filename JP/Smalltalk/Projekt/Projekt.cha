<?xml version="1.0"?><st-source><component-created><name>Wielokaty</name> <type>package</type></component-created><class><name>Wielokat</name><environment>Smalltalk</environment><super>Core.Object</super><private>false</private><indexed-type>none</indexed-type><inst-vars>wierzcholki nazwa </inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>JezykiProgramowania</category><attributes><package>Wielokaty</package></attributes></class><do-it>Object subclass: #Wielokat	instanceVariableNames: 'wierzcholki nazwa '	classVariableNames: ''	poolDictionaries: ''	category: 'JezykiProgramowania'</do-it><methods><class-id>Wielokat</class-id> <category>initialize-release</category><body package="Wielokaty" selector="initialize:name:">initialize: liczbaWierzcholkow name: nowaNazwa	"konstruktor obiektu - wielokata"	nazwa:=nowaNazwa.	wierzcholki:=Array new: liczbaWierzcholkow.	wierzcholki at: 1 put: 0@0.</body></methods><methods><class-id>Wielokat</class-id> <category>accessing</category><body package="Wielokaty" selector="nazwa">nazwa	"podaje nazwe wielokata"	^nazwa</body></methods><methods><class-id>Wielokat</class-id> <category>accessing</category><body package="Wielokaty" selector="nazwa:">nazwa: nowa_nazwa	"ustawia nowa nazwe wielokata"	nazwa:=nowa_nazwa</body></methods><class><name>Kwadrat</name><environment>Smalltalk</environment><super>Wielokat</super><private>false</private><indexed-type>none</indexed-type><inst-vars></inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>JezykiProgramowania</category><attributes><package>Wielokaty</package></attributes></class><do-it>Wielokat subclass: #Kwadrat	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'JezykiProgramowania'</do-it><methods><class-id>Kwadrat</class-id> <category>arithmetic</category><body package="Wielokaty" selector="+">+ figura	"dodaj 2 figury w sensie pola"	| p |	p:=self pole + figura pole.	^(Kwadrat new) initialize: p sqrt</body></methods><methods><class-id>Kwadrat</class-id> <category>actions</category><body package="Wielokaty" selector="pole">pole	"licz pole kwadratu"	^(wierzcholki at: 2) x squared</body></methods><methods><class-id>Kwadrat</class-id> <category>initialize-release</category><body package="Wielokaty" selector="initialize:">initialize: bok	"tworz kwadrat o podanym boku"	super initialize: 4 name: 'Kwadrat'.	wierzcholki at: 2 put: bok@0.	wierzcholki at: 3 put: bok@bok.	wierzcholki at: 4 put: 0@bok.</body></methods><do-it>Kwadrat renameTo: #TrojkatRownoramienny</do-it><methods><class-id>TrojkatRownoramienny</class-id> <category>arithmetic</category><body package="Wielokaty" selector="+">+ figura	"dodaj 2 figury w sensie pola"	| p |	p:=self pole + figura pole.	^(TrojkatRownoramienny new) initialize: p sqrt</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>actions</category><body package="Wielokaty" selector="pole">pole	"licz pole trójkąta"	^(wierzcholki at: 2) x squared</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>actions</category><body package="Wielokaty" selector="drukuj">drukuj	"drukuj informacje o figurze"	| a |	</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>initialize-release</category><body package="Wielokaty" selector="initialize:">initialize: bok	"tworz kwadrat o podanym boku"	super initialize: 3 name: 'TrojkatRownoramienny'.	wierzcholki at: 2 put: bok@0.	wierzcholki at: 3 put: bok@bok.	wierzcholki at: 4 put: 0@bok.</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>initialize-release</category><body package="Wielokaty" selector="initialize:and:">initialize: podstawa and: wysokosc	"tworz trójkąt równoramienny o podanej podstawie i wysokości"	super initialize: 3 name: 'TrojkatRownoramienny'.	wierzcholki at: 2 put: podstawa@0.	wierzcholki at: 3 put: (podstawa/2)@wysokosc.</body></methods><remove-selector><class-id>TrojkatRownoramienny</class-id> <selector>initialize:and:</selector></remove-selector><methods><class-id>TrojkatRownoramienny</class-id> <category>initialize-release</category><body package="Wielokaty" selector="initialize:and:">initialize: podstawa and: wysokosc	"tworz trójkąt równoramienny o podanej podstawie i wysokości"	super initialize: 3 name: 'TrojkatRownoramienny'.	wierzcholki at: 2 put: podstawa@0.	wierzcholki at: 3 put: (podstawa/2)@wysokosc.</body></methods><remove-selector><class-id>TrojkatRownoramienny</class-id> <selector>initialize:</selector></remove-selector><methods><class-id>TrojkatRownoramienny</class-id> <category>actions</category><body package="Wielokaty" selector="pole">pole	"licz pole trójkąta równoramiennego"	^(wierzcholki at: 2)  * ((wierzcholki at: 3)/2)</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>actions</category><body package="Wielokaty" selector="pole">pole	"licz pole trójkąta równoramiennego"	^((wierzcholki at: 2)  x) * ((wierzcholki at: 3)  y / 2)</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>actions</category><body package="Wielokaty" selector="drukuj">drukuj	"drukuj informacje o figurze"	Transcript show: wierzcholki.	Transcript show: self pole</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>actions</category><body package="Wielokaty" selector="drukuj">drukuj	"drukuj informacje o figurze"	wierzcholki do: [ :p | Transcript show: p ].	Transcript show: self pole</body></methods><methods><class-id>TrojkatRownoramienny</class-id> <category>actions</category><body package="Wielokaty" selector="drukuj">drukuj	"drukuj informacje o figurze"	wierzcholki do: [ :p | Transcript show: (p x printString), '@', (p y printString) ].	Transcript show: self pole</body></methods><methods>